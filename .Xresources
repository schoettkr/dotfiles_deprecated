Xcursor.theme: DMZ-White
URxvt*buffered: false

URxvt.font:                xft:Inconsolata:pixelsize=16:bold
URxvt.boldFont:            xft:Inconsolata:pixelsize=18:bold
URxvt.italicFont:          xft:Inconsolata:pixelsize=16:italic
URxvt.boldItalicFont:      xft:Inconsolata:pixelsize=18:bold:italic
