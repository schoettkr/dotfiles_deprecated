URxvt.letterSpace:         -1

/* URxvt.transparent:   true */
/* URxvt.shading:       40 */
*.cursorColor:  #d3d0c8 */
*.background:   #263238
*.foreground:   #f2f0ec
!black
*color0:  #222222
*color8:  #666666
!red
*color1:  #E84F4F
*color9:  #D23D3D
!green
*color2:  #B7CE42
*color10: #BDE077
!yellow
*color3:  #FEA63C
*color11: #FFE863
!blue
*color4:  #66AABB
*color12: #AACCBB
!magenta
*color5:  #B7416E
*color13: #E16A98
!cyan
*color6:  #6D878D
*color14: #42717B
!white
*color7:  #DDDDDD
*color15: #CCCCCC

URxvt.internalBorder:      0
URxvt.externalBorder:      0

URxvt.scrollBar:           false
URxvt.lineSpace: 1

URxvt*fading: 30
URxvt.urgentOnBell: false
URxvt*visualBell: false

